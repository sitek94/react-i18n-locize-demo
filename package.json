{
  "name": "react-i18n-demo",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview --port 8080",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "lint": "eslint --ext ts,tsx src/",
    "lint:fix": "npm run lint -- --fix",
    "locize:sync": "source .env.local && locize sync --ver=latest --path=./public/locales --project-id=$VITE_LOCIZE_PROJECT_ID --api-key=$VITE_LOCIZE_API_KEY",
    "locize:sync:update-values": "yarn locize:sync --update-values=true --reference-language-only=false"
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "i18next": "^21.6.10",
    "i18next-browser-languagedetector": "^6.1.3",
    "i18next-locize-backend": "^4.2.8",
    "locize-lastused": "^3.0.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-react": "^1.0.7",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-tsconfig-paths": "^3.3.17"
  }
}
